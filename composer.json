{
  "name": "tourze/tls-session",
  "description": "A PHP library for TLS session management and maintenance, providing comprehensive session handling, ticket management, and resumption capabilities for TLS 1.2 and TLS 1.3 protocols",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-hash": "*",
    "ext-json": "*",
    "ext-openssl": "*",
    "tourze/tls-common": "0.0.*",
    "tourze/tls-crypto-hash": "0.0.*",
    "tourze/tls-crypto-symmetric": "0.0.*",
    "tourze/tls-handshake-flow": "0.0.*",
    "tourze/tls-x509-core": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TLSSession\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TLSSession\\Tests\\": "tests/"
    }
  }
}